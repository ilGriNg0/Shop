<Application x:Class="Tretiy.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Tretiy"
             StartupUri="MainWindow.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ButtonStyles.xaml"/>
                <ResourceDictionary Source="BorderStyles.xaml"/>
                <ResourceDictionary Source="ToggleButtonStyles.xaml"/>
                <ResourceDictionary Source="DatePickerStyles.xaml"/>
                <ResourceDictionary Source="TextBoxStyles.xaml"/>
                <ResourceDictionary Source="TextBlockStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
